<?xml version="1.0" encoding="UTF-8"?>
<!-- Copy this file to ~/.m2/toolchains.xml and edit the <jdkHome> path so Maven can use JDK 17 for builds -->
<toolchains>
  <toolchain>
    <type>jdk</type>
    <provides>
      <version>17</version>
      <!-- Vendor optional: omit or set to your JDK vendor (temurin, corretto, oracle, etc.) -->
    </provides>
    <configuration>
      <!-- Update jdkHome to your local JDK 17 installation path -->
      <jdkHome>/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home</jdkHome>
    </configuration>
  </toolchain>
</toolchains>

